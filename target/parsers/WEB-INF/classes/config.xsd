<?xml version="1.0" encoding="UTF-8" ?>
<tn:schema xmlns:tn="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.xml.tc/gem"
           xmlns:tns="http://www.xml.tc/gem">

    <tn:element name="gems">
        <tn:complexType>
            <tn:sequence>
                <tn:element name="gem" type="tns:Gem" minOccurs="1" maxOccurs="unbounded"/>
            </tn:sequence>
        </tn:complexType>
    </tn:element>


    <tn:complexType name="Gem">
        <tn:attribute name="id" type="tn:int" use="required"/>
        <tn:sequence>
            <tn:element name="name" type="tn:string"/>
            <tn:element name="preciousness" type="tn:string"/>
            <tn:element name="visualParameters" type="tns:Visual"/>
            <tn:element name="value" type="tn:positiveInteger"/>
        </tn:sequence>
    </tn:complexType>
    <tn:complexType name="Visual">
        <tn:sequence>
            <tn:element name="color" type="tn:string"/>
            <tn:element name="shine" type="tn:string"/>
            <tn:element name="transparency" type="tn:string"/>
        </tn:sequence>
    </tn:complexType>

</tn:schema>